{ "name": "jsonldstore"
, "version": "0.0.1"
, "author": "Ryan Shaw <ryanshaw@unc.edu>"
, "description": "a RESTful service for storing and managing JSON-JD"
, "bin": { "jsonldstore": "./bin/jsonldstore" }
